{
  "name": "fortnitestats",
  "productName": "Fortnite Stats",
  "version": "3.0.0",
  "description": "A Fortnite Stats application to see your stats live",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . fortnitestats --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=\"Jack's Corp\" --version-string.FileDescription=CE --version-string.ProductName=\"Fortnite Stats\"",
    "package-linux": "electron-packager . fortnitestats --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "By_Jack",
  "license": "ISC",
  "devDependencies": {
    "electron": "^2.0.0-beta.8",
    "electron-packager": "^12.0.1"
  },
  "dependencies": {
    "discord-rich-presence": "0.0.7",
    "express": "^4.16.3",
    "form-data": "^2.3.2",
    "jquery": "^3.3.1",
    "node-fetch": "latest",
    "number-formatter": "^1.2.0",
    "update-electron-app": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jacxk/FortniteStatsTracker.git"
  },
  "keywords": [
    "Fortnite",
    "stats",
    "tracker",
    "fortnite",
    "stats",
    "fornite",
    "tracker"
  ],
  "bugs": {
    "url": "https://github.com/Jacxk/FortniteStatsTracker/issues"
  },
  "homepage": "https://github.com/Jacxk/FortniteStatsTracker#readme"
}
